generator client {
  provider = "prisma-client-js"
}

generator erd {
  provider = "prisma-erd-generator"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Car {
  id          String @id @default(cuid())
  brand       String
  model       String
  pricePerDay Int
  pricePerKm  Int
  maxDuration Int
  maxDistance Int
}
